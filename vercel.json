{
  "rewrites": [
    {
      "source": "/stories/:slug",
      "destination": "/stories/:slug/index.html"
    },
    {
      "source": "/stories/:slug/chapter/:number",
      "destination": "/stories/:slug/chapter/:number/index.html"
    },
    {
      "source": "/comics/:id",
      "destination": "/comics/:id/index.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*/index.html"
    },
    {
      "source": "/catalogue",
      "destination": "/catalogue/index.html"
    },
    {
      "source": "/leaderboard",
      "destination": "/leaderboard/index.html"
    },
    {
      "source": "/login",
      "destination": "/login/index.html"
    },
    {
      "source": "/register",
      "destination": "/register/index.html"
    },
    {
      "source": "/profile",
      "destination": "/profile/index.html"
    },
    {
      "source": "/write",
      "destination": "/write/index.html"
    },
    {
      "source": "/((?!_astro|_next|favicon|manifest|robots).*)",
      "destination": "/index.html"
    }
  ]
}
