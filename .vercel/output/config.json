				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/admin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/bd\\/([^/]+?)\\/chapter\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/bd\\/([^/]+?)\\/read\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/bd\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/catalogue\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/comics\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/comics\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/leaderboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/novel\\/([^/]+?)\\/chapter\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/novel\\/([^/]+?)\\/read\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/novel\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/novels\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/profile\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/register\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/stories\\/([^/]+?)\\/add-chapter\\/?$",
			"dest": "_render"
