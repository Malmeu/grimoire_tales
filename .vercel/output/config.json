		{
			"src": "^\\/stories\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/write\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}